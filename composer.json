{
    "name": "st2f/phpunit",
    "description": "Basic phpunit starter",
    "license": "MIT",
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/http-foundation": "^7.2"
    }
}
